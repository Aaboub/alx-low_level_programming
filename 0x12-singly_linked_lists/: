#include <string.h>

/**
 * print_list - entry
 * @h: param
 *
 * Return: size
 */
size_t print_list(const list_t *h)
{
	size_t size = 0;

	if (h == NULL)
	{
		printf("[0] (nil)");
		return (0);
	}
	while (h.next != NULL)
	{
		if (h.str != NULL)
			printf("[%d] %s", strlent(h.str), h.str);
		else
			printf("[0] (nil)");
		size++;
	}
	return (size);
}
